{
  "hosting": [
      {
      "target": "rookout-docs",
      "public": "website/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [
        {
          "source": "/docs/sdk-setup.html",
          "destination": "/docs/setup-intro/setup-intro.html",
          "type": 301
        }
      ],
      "headers": [
        {
          "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
          "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=604800"
          } ]
        },
        {
          "source" : "**/*.@(html|js|css)",
          "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=3600"
          } ]
        }
      ]
    },
    {
      "target": "rookout-dci-docs",
      "public": "website/build/docs",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [
        {
          "source": "/docs/sdk-setup.html",
          "destination": "/docs/setup-intro/setup-intro.html",
          "type": 301
        }
      ],
      "headers": [
        {
          "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
          "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=604800"
          } ]
        },
        {
          "source" : "**/*.@(html|js|css)",
          "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=3600"
          } ]
        }
      ]
    }
  ]
}
